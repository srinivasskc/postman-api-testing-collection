							"name": "Add Calculator_Environment Variables",
							"event": [
								{
									"listen": "test",
									"script": {
										"id": "11fc52c8-4ae6-4930-9f72-3313a4ca80f2",
										"exec": [
											"pm.test(\"Using XML to JSON\", function () {\r",
											"    var jsonObject = xml2Json(responseBody);\r",
											"    pm.expect(jsonObject['soap:Envelope']['soap:Body']['AddResponse']['AddResult']).to.eql(\"6\");\r",
											"}); \r",
											"\r",
											""
										],
										"type": "text/javascript"
									}
								}
							],
							"protocolProfileBehavior": {
								"disabledSystemHeaders": {
									"content-type": true
								}
							},
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "Content-Type",
										"value": "text/xml",
										"type": "text"
									}
								],
								"body": {
									"mode": "raw",
									"raw": "<?xml version=\"1.0\" encoding=\"utf-8\"?>\r\n<soap:Envelope xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema\" xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\">\r\n  <soap:Body>\r\n    <Add xmlns=\"http://tempuri.org/\">\r\n      <intA>{{add_a}}</intA>\r\n      <intB>{{add_b}}</intB>\r\n    </Add>\r\n  </soap:Body>\r\n</soap:Envelope>",
									"options": {
										"raw": {
											"language": "xml"
										}
									}
								},
								"url": {
									"raw": "http://www.dneonline.com/calculator.asmx",
									"protocol": "http",
									"host": [
										"www",
										"dneonline",
										"com"
									],
									"path": [
										"calculator.asmx"
									]
								}
							},
							"response": []
						},
						{
							"name": "Add Calculator_Hardcoded Values",
							"event": [
								{
									"listen": "test",
									"script": {
										"id": "6b3a3001-8494-44c9-a57e-b2f6d7f0cfed",
										"exec": [
											"pm.test(\"Test Using XML to JSON\", function () {\r",
											"    var jsonObject = xml2Json(responseBody);\r",
											"    pm.expect(jsonObject['soap:Envelope']['soap:Body']['AddResponse']['AddResult']).to.eql(\"7\");\r",
											"}); \r",
											"\r",
											"\r",
											"pm.test(\"Test Using Cheerio\", function () {\r",
											"    const $ = cheerio.load(responseBody)\r",
											"    pm.expect($('AddResult').text()).to.eql(\"7\");\r",
											"    pm.expect($('AddResponse').attr('xmlns')).to.eql(\"http://tempuri.org/\");\r",
